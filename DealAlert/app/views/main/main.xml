<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded">
  <StackLayout>
	<ListView items="{{dealItems}}"
	              itemTap="{{onItemTapped}}">
	      <ListView.itemTemplate>
	        <StackLayout>
	          <Label text="{{Title}}" />
	        </StackLayout>
	      </ListView.itemTemplate>
	    </ListView>
	  </StackLayout>
</Page>
